{
  "name": "projec",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "create:module": "node /scripts/createModule.js",
    "create:test:e2e": "node /scripts/generate-e2e-feature.js",
    "lint": "eslint \"{src}/**/*.ts\"",
    "lint:fix": "eslint '{src}/**/*.ts' --fix",
    "lint:format": "eslint --fix \"src/**/*.ts\"",
    "db:migrate": "sequelize-cli db:migrate --config src/database/config/sequelize-cli-config.js --migrations-path src/database/migrations",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --config src/database/config/sequelize-cli-config.js --migrations-path src/database/migrations",
    "db:seed": "sequelize-cli db:seed:all --config src/database/config/sequelize-cli-config.js --seeders-path src/database/seeders",
    "db:seed:undo": "sequelize-cli db:seed:undo:all --config src/database/config/sequelize-cli-config.js --seeders-path src/database/seeders",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cucumber-js",
    "test:e2e:cov": "c8 --reporter=text --reporter=lcov pnpm test:e2e",
    "docs:serve": "npx @compodoc/compodoc -p tsconfig.build.json -s --host 0.0.0.0 --port 80 src",
    "docs:build": "npx @compodoc/compodoc -p tsconfig.build.json -d documentation src",
    "docs:pdf": "node generate-docs-pdf.js",
    "docs:generate:pdf": "pnpm run docs:build && pnpm run docs:pdf"
  },
  "dependencies": {
    "@bdd-backend/common": "1.12.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/sequelize": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csrf-csrf": "^4.0.3",
    "dotenv-expand": "^12.0.3",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "nestjs-pino": "^4.4.1",
    "passport": "^0.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.32",
    "@cucumber/cucumber": "^12.2.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "c8": "^10.1.3",
    "dotenv": "^17.2.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jest": "^30.2.0",
    "mysql2": "^3.15.3",
    "pino-colada": "^2.2.2",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "puppeteer": "^24.29.1",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "database",
      "node_modules",
      "dist",
      "test"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>src/context/$1"
    }
  }
}
